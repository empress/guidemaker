{
  "name": "guidemaker",
  "version": "3.3.1",
  "description": "Guidemaker Core Engine",
  "keywords": [
    "ember-addon",
    "ember-cli-netlify-plugin",
    "prember-plugin"
  ],
  "repository": "https://github.com/empress/guidemaker",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:node": "mocha node-tests",
    "ember-compatibility-tests": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^3.0.1",
    "broccoli-merge-trees": "^4.1.0",
    "broccoli-static-site-json": "^4.5.0",
    "compare-versions": "^3.5.0",
    "ember-auto-import": "^2.6.3",
    "ember-body-class": "^3.0.0",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-cli-showdown": "^9.0.0",
    "ember-get-config": "^2.1.1",
    "ember-meta": "^2.0.0",
    "ember-scroll": "^1.0.3",
    "empress-blueprint-helpers": "^1.2.0",
    "js-yaml": "^3.13.1",
    "jsonapi-serializer": "^3.6.4",
    "resolve": "^1.11.1",
    "showdown-section-groups": "^0.3.0",
    "walk-sync": "^2.0.2",
    "yaml-front-matter": "^4.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.0",
    "@embroider/test-setup": "^3.0.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "chai": "^4.2.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.4.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-fastboot": "^4.1.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~5.3.0",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.0.0",
    "ember-qunit": "^8.0.1",
    "ember-resolver": "^11.0.1",
    "ember-source": "~5.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-ember": "^11.11.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-qunit": "^8.0.1",
    "guidemaker-default-template": "^7.0.0",
    "lint-to-the-future": "^1.1.0",
    "lint-to-the-future-eslint": "^1.0.0",
    "loader.js": "^4.7.0",
    "mocha": "^7.0.0",
    "prember": "^2.0.0",
    "prettier": "^3.0.3",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "release-plan": "^0.8.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.2",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-source": ">= 3.28.0",
    "showdown": "^2.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": [
      "ember-meta",
      "ember-data"
    ]
  },
  "fastbootDependencies": [
    "algoliasearch",
    "crypto"
  ]
}
